{
  "name": "ng-pick-datetime",
  "version": "0.0.0",
  "description": "Angular Date Time Picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker"
  ],
  "author": "daniel pan <myron.yk.pan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build picker",
    "test": "ng test picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build picker && cp README.md dist/picker",
    "build_css": "mkdir -p dist/picker/assets/style && node-sass --output-style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
    "npm_pack": "cd dist/picker && npm pack",
    "package": "npm run build_lib && npm run build_css && npm run npm_pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielYKPan/date-time-picker.git"
  },
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/localize": "^12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@angular/service-worker": "^12.2.17",
    "core-js": "^2.6.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "rxjs": "~6.5.3",
    "tailwindcss": "^2.2.19",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "^12.2.17",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.13.7",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^12.2.7",
    "node-sass": "^4.12.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.1.0",
    "tslib": "^2.4.1",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  }
}
